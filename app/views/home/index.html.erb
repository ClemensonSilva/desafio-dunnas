<% if current_user %>
       <p>Hello <%= current_user&.email %></p><br>
       <p>You are Role: <%= current_user&.role %></p><br>
       <% if current_user.role %>
         <p>User Total count <%= User.count %></p>
       <% end %>
       <% if can? :all, Funcionario %>
         <%= link_to 'Crie uma nova unidade', new_unidade_path %>
       <% end %> <br>
        <% if can? :all, Funcionario %>
          <%= link_to 'Crie um novo setor', new_setor_path %>
        <% end %> <br>
